{
  "name": "lixso-game",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:4200",
    "lint": "npm run lint:ts && npm run lint:html && npm run lint:scss && npm run lint:format",
    "lint:ts": "eslint \"src/**/*.ts\" \"e2e/**/*.ts\"",
    "lint:html": "eslint \"src/**/*.html\" && htmlhint \"src/**/*.html\"",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:format": "prettier --check \"src/**/*.{ts,html,scss,css,json}\" \"e2e/**/*.ts\" \"*.{json,md}\"",
    "lint:fix": "npm run lint:ts:fix && npm run lint:html:fix && npm run lint:scss:fix && npm run format",
    "lint:ts:fix": "eslint \"src/**/*.ts\" \"e2e/**/*.ts\" --fix",
    "lint:html:fix": "eslint \"src/**/*.html\" --fix",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss,css,json}\" \"e2e/**/*.ts\" \"*.{json,md}\"",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "openai": "^6.8.0",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
    "tone": "^15.1.22",
    "tslib": "^2.3.0",
    "webgazer": "^3.4.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.0.1",
    "@analogjs/vitest-angular": "^2.0.1",
    "@angular-eslint/eslint-plugin": "^20.5.1",
    "@angular-eslint/eslint-plugin-template": "^20.5.1",
    "@angular-eslint/template-parser": "^20.5.1",
    "@angular/build": "^20.3.8",
    "@angular/cli": "^20.3.8",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/platform-browser-dynamic": "^20.3.9",
    "@playwright/test": "^1.56.1",
    "@testing-library/angular": "^18.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "happy-dom": "^20.0.10",
    "htmlhint": "^1.7.1",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "stylelint": "^16.25.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^6.12.1",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4"
  }
}
